<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AirClipCCL.Views.TrimDetails">
    <StackPanel Margin="5">
        <StackPanel Orientation="Horizontal" Spacing="10" Margin="5">
            <TextBlock Text="Start Time" VerticalAlignment="Center"/>
            <TextBox FontSize="10" MinHeight="1"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal" Spacing="10" Margin="5">
            <TextBlock Text="End Time" VerticalAlignment="Center"/>
            <TextBox FontSize="10" MinHeight="1"/>
        </StackPanel>
    </StackPanel>
</UserControl>
