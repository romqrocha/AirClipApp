<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewmodels="clr-namespace:AirClipApp.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AirClipApp.Views.ImportPage"
             x:DataType="viewmodels:ImportPageViewModel">
    
    <StackPanel Margin="20">
        <TextBlock Margin="0 5" Text="{Binding ImportPrompt}"/>
        <Button Margin="0 5" Click="OnImport" Content="Import Video"/>
        <TextBlock Name="ImportStatus" Margin="0 5" Text="{Binding ImportStatus}"/>
    </StackPanel>
</UserControl>
